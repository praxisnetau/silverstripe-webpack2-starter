{
  "name": "praxisnetau/silverstripe-webpack2-starter",
  "description": "Starter kit for SilverStripe 4 and Webpack 2.",
  "homepage": "https://github.com/praxisnetau/silverstripe-webpack2-starter",
  "keywords": [
    "silverstripe",
    "bootstrap",
    "webpack2",
    "webpack",
    "starter",
    "theme"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Colin Tucker",
      "role": "Developer",
      "email": "colin@praxis.net.au",
      "homepage": "http://www.praxis.net.au"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "silverstripe/cms": "4.0.x-dev",
    "silverstripe/framework": "4.0.x-dev",
    "silverstripe/reports": "4.0.x-dev",
    "silverstripe/siteconfig": "4.0.x-dev",
    "silverstripe/asset-admin": "1.0.x-dev",
    "silverstripe/graphql": "0.2.x-dev",
    "gdmedia/ss-auto-git-ignore": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "scripts": {
    "post-update-cmd": [
      "GDM\\SSAutoGitIgnore\\UpdateScript::Go"
    ]
  },
  "config": {
    "process-timeout": 600
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
